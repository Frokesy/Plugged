{
    "name": "paginato",
    "private": false,
    "version": "0.2.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
        "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
        "prepare": "husky install"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": [
            "yarn  eslint",
            "yarn  format"
        ]
    },
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/paginator.es.js",
            "require": "./dist/paginator.umd.js"
        },
        "./dist/paginator.css": "./dist/paginator.css"
    },
    "main": "./dist/paginator.umd.js",
    "module": "./dist/paginator.es.js",
    "types": "./dist/index.d.ts",
    "devDependencies": {
        "@types/node": "^18.7.6",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "@typescript-eslint/eslint-plugin": "^5.34.0",
        "@typescript-eslint/parser": "^5.34.0",
        "@vitejs/plugin-react": "^2.1.0",
        "autoprefixer": "^10.4.20",
        "eslint": "^8.2.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.30.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "postcss": "^8.4.41",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tailwindcss": "^3.4.10",
        "typescript": "^4.6.4",
        "vite": "^3.1.0",
        "vite-plugin-dts": "^1.4.1",
        "vite-plugin-linter": "^1.2.0",
        "vite-tsconfig-paths": "^3.5.0"
    },
    "peerDependencies": {
        "react": "16.8.0 || >=17.x",
        "react-dom": "16.8.0 || >=17.x"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Frokesy/paginato"
    },
    "keywords": [],
    "author": "Ayanfeoluwa Akindele",
    "description": "A simple and customizable paginator component for React.",
    "bugs": {
        "url": "https://github.com/Frokesy/paginato/issues"
    },
    "homepage": "https://github.com/Frokesy/paginato#readme",
    "license": "MIT"
}